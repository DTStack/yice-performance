{
    "name": "yice-performance",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build",
        "build:test": "NODE_ENV=staging turbo run build",
        "start": "NODE_ENV=production nest start",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "deploy": "zx scripts/deploy.mjs",
        "deploy:test": "npm run deploy test",
        "static": "bash scripts/static.command",
        "static:test": "bash scripts/static-test.command",
        "pm2": "pm2 restart ./pm2/config.json --env production",
        "pm2:test": "pm2 restart ./pm2/config-test.json --env staging"
    },
    "dependencies": {
        "echarts": "^5.5.0",
        "lodash": "^4.17.21",
        "moment": "^2.30.1"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.191",
        "cz-conventional-changelog": "^3.3.0",
        "ts-loader": "^9.2.3",
        "tsconfig-paths": "4.1.1",
        "turbo": "^2.0.3",
        "typescript": "4.9.5",
        "webpack": "^5.0.0",
        "zx": "^7.2.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "packageManager": "pnpm@6.32.17",
    "engines": {
        "node": ">=14"
    }
}
